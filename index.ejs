<% include ../includes/start.ejs %>
<img class="logo" src="/images/logo.png"/>
<div class="interests <%= user.interested %>">
	<img class="male" src="/images/male.png"/>
	<img class="female" src="/images/female.png"/>
</div>
<div class="container">
	<div class="header">
		Which photo looks better?
	</div>
	<div class="cards">
		<div class="card left">
			<div class="results"></div>
		</div>
		<div class="card right">
			<div class="results"></div>
		</div>
		<div class="vs">OR</div>
		<div class="keys left">
			<div class="key">1</div>
			<div class="key">A</div>
			<div class="key">W</div>
			<div class="key">←</div>
			<div class="key">↑</div>
		</div>
		<div class="keys right">
			<div class="key">2</div>
			<div class="key">D</div>
			<div class="key">S</div>
			<div class="key">→</div>
			<div class="key">↓</div>
		</div>
	</div>
</div>
